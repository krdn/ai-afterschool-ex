{
  "name": "ai-afterschool-ex",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:seed": "turbo run db:seed",
    "dev": "turbo run dev",
    "build:docker": "docker build -t ai-afterschool-web:local -f apps/web/Dockerfile --target production .",
    "build:docker:migrate": "docker build -t ai-afterschool-migrate:local -f apps/web/Dockerfile --target migrate .",
    "test:deploy": "pnpm build:docker && docker run --rm -p 3000:3000 -e DATABASE_URL=postgresql://dummy:dummy@localhost:5432/dummy -e SESSION_SECRET=local-test-secret-at-least-32-chars ai-afterschool-web:local",
    "fix:packages": "syncpack fix && syncpack format",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "lint": "turbo run lint",
    "lint:packages": "syncpack lint",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^10.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.4.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3",
    "syncpack": "^14.0.0",
    "turbo": "^2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=24"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "@prisma/engines",
      "prisma"
    ]
  }
}